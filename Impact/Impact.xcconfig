#include "xcconfigs/UniversalFramework_Framework.xcconfig"

APPLICATION_EXTENSION_API_ONLY = YES
MACH_O_TYPE = staticlib
SWIFT_VERSION = 5.0

PRODUCT_NAME = Impact
PRODUCT_BUNDLE_IDENTIFIER = com.stacksift.Impact
PRODUCT_MODULE_NAME = Impact
CURRENT_PROJECT_VERSION = 12
DYLIB_CURRENT_VERSION = $(CURRENT_PROJECT_VERSION)
MARKETING_VERSION = 0.3.8

INFOPLIST_FILE = Impact/Info.plist

MACOSX_DEPLOYMENT_TARGET = 10.13
IPHONEOS_DEPLOYMENT_TARGET = 12.0
TVOS_DEPLOYMENT_TARGET = 12.0
WATCHOS_DEPLOYMENT_TARGET = 4.0

GCC_PREPROCESSOR_DEFINITIONS = $(inherited) CURRENT_PROJECT_VERSION=$(CURRENT_PROJECT_VERSION)
